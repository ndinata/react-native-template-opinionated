{
  "name": "MyProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "git config --get core.hooksPath || husky install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "pod-install": "bundle exec pod install --project-directory=./ios/",
    "test": "jest",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "e2e:ios:debug": "detox build -c ios.debug && detox test -c ios.debug",
    "e2e:ios:release": "detox build -c ios.release && detox test -c ios.release",
    "e2e:android:debug": "detox build -c android.debug && detox test -c android.debug",
    "e2e:android:release": "detox build -c android.release && detox test -c android.release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.5",
    "@react-native-community/netinfo": "^9.3.0",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "luxon": "^3.3.0",
    "react": "18.2.0",
    "react-native": "0.71.14",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.3.0",
    "react-native-svg": "^13.2.0",
    "react-native-version-number": "^0.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/react-native": "^12.3.2",
    "@types/jest": "^29.2.1",
    "@types/luxon": "^3.3.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^19.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "jest-circus": "^29.2.1",
    "lint-staged": "^11.2.6",
    "metro-react-native-babel-preset": "0.73.10",
    "prettier": "^3.0.3",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.3"
  }
}
