{
  "name": "MyProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "git config --get core.hooksPath || husky install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "e2e:ios:debug": "detox build -c ios.debug && detox test -c ios.debug",
    "e2e:ios:release": "detox build -c ios.release && detox test -c ios.release",
    "e2e:android:debug": "detox build -c android.debug && detox test -c android.debug",
    "e2e:android:release": "detox build -c android.release && detox test -c android.release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.5",
    "@react-native-community/netinfo": "^7.1.1",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "dayjs": "^1.8.35",
    "react": "17.0.2",
    "react-native": "0.67.1",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^3.3.0",
    "react-native-svg": "^12.1.1",
    "react-native-version-number": "^0.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^27.0.2",
    "@types/react-native": "^0.66.1",
    "@types/react-test-renderer": "^17.0.0",
    "babel-jest": "^27.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^19.0.0",
    "eslint": "7.14.0",
    "husky": "^7.0.4",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-rn": "^1.1.5",
    "jest": "^27.3.1",
    "jest-circus": "^27.3.1",
    "lint-staged": "^11.2.6",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.1.2",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "import-sort --write",
      "prettier --config .prettierrc.json --write",
      "eslint"
    ]
  },
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "rn"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "rn"
    }
  }
}
