{
  "root": true,
  "extends": ["@react-native-community", "plugin:prettier/recommended"],
  "overrides": [
    {
      "files": ["*.js", "*.ts", "*.tsx"],
      "rules": {
        "no-console": 1,
        "no-undef": 0,
        "react-hooks/exhaustive-deps": 1,
        "react-native/no-unused-styles": 1,
        "@typescript-eslint/no-unused-vars": 1
      }
    }
  ]
}
