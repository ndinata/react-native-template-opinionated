{
  "root": true,
  "extends": ["@react-native", "plugin:prettier/recommended"],
  "overrides": [
    {
      "files": ["*.js", "*.jsx", "*.ts", "*.tsx"],
      "rules": {
        "dot-notation": "off",
        "no-console": "warn",
        "no-undef": "off",
        "react-hooks/exhaustive-deps": "warn",
        "react-native/no-unused-styles": "warn",
        "@typescript-eslint/no-unused-vars": "warn"
      }
    }
  ]
}
